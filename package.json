{
  "name": "react-boilerplate",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config internals/webpack/webpack.dev.babel.js",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js",
    "start:dev": "dotenv -e .env.dev -- node server",
    "start:qa": "dotenv -e .env.qa -- node server",
    "migrate:qa": "dotenv -e .env.qa -- npx sequelize-cli db:migrate --env qa",
    "seed:qa": "dotenv -e .env.qa -- npx sequelize-cli db:seed:all --env qa"
  },
  "dependencies": {
    "@mui/icons-material": "^5.14.7",
