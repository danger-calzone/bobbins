    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "chalk": "^5.6.2",
    "circular-dependency-plugin": "^5.2.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "fontfaceobserver": "^2.3.0",
    "html-webpack-plugin": "^5.6.4",
    "invariant": "^2.2.4",
    "jwt-decode": "^4.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
